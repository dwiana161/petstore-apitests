{
	"info": {
		"_postman_id": "4b5e677e-c533-4c32-b6ba-dfe03ec15b07",
		"name": "SwaggerAPI",
		"description": "Petstore API Collection\n\nThis is a sample server Petstore server. The collection contains endpoints to manage pets",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
